library(tidyverse)
df <- readxl::read_xlsx("corpus_search_results.xlsx")
df |> 
  add_count(query) |> 
  arrange(-n) |> 
  filter(n == 1) |> 
  writexl::write_xlsx("corpus_search_results_unique.xlsx")

